CREATE TABLE `tb_permissao_login` (
  `id_permissao_login` int NOT NULL AUTO_INCREMENT,
  `fk_id_permissao` int NOT NULL,
  `fk_id_login` int NOT NULL,
  PRIMARY KEY (`id_permissao_login`),
  KEY `fk_id_permissao_idx` (`fk_id_permissao`),
  KEY `fk_id_login_idx` (`fk_id_login`),
  CONSTRAINT `fk_id_login` FOREIGN KEY (`fk_id_login`) REFERENCES `tb_login` (`id_login`),
  CONSTRAINT `fk_id_permissao` FOREIGN KEY (`fk_id_permissao`) REFERENCES `tb_permissao` (`id_permissao`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci